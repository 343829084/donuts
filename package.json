{
  "name": "donuts",
  "version": "0.0.7",
  "description": "cli interface for donuts",
  "main": "bin/donuts.js",
  "preferGlobal": "true",
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "preinstall": "(which cordova) || npm install -g cordova; (which bower) || npm install bower -g; (which cordova-icon) || npm install cordova-icon -g; (which cordova-splash) || npm install cordova-splash -g"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sofish/donuts.git"
  },
  "keywords": [
    "donuts",
    "hybrid",
    "html5"
  ],
  "author": "sofish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sofish/donuts/issues"
  },
  "homepage": "https://github.com/sofish/donuts",
  "dependencies": {
    "chalk": "^1.1.1",
    "commander": "^2.8.1",
    "progress": "^1.1.8"
  }
}
